{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "downlevelIteration": true,
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "importHelpers": true,
        "target": "ES2022",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "ESNext",
            "dom"
        ],
        "useDefineForClassFields": false
    },
    // "angularCompilerOptions": {
